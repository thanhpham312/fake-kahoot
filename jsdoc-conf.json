{
  "plugins": [
    "plugins/summarize",
    "plugins/markdown",
    "./node_modules/jsdoc-http-plugin"
  ],
  "recurseDepth": 20,
  "source": {
    "include": [
      "./controllers/",
      "./models/",
      "./public/",
      "./app.js"
    ],
    "exclude": [
      "./public/assets/",
      "./public/styles/",
      "./node_modules/",
      "./public/javascripts/timer.js"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "template": "templates/default",
    "recurse": true,
    "destination": "./out/",
    "encoding": "utf8"
  }
}

